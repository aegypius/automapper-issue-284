{
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "aegypius",
            "email": "git@aegypius.com"
        }
    ],
    "scripts": {},
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.3",
        "jolicode/automapper": "^9.4.1"
    },
    "require-dev": {
        "ext-dom": "*",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan": "^2.1.17",
        "phpunit/phpunit": "^12.2.2",
        "symfony/var-dumper": "^7.3",
        "symplify/easy-coding-standard": "^12.5.20"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
